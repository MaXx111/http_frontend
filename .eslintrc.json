{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
          ],
          "import/extensions": [
            "error",
            "ignorePackages"
            ],
          "class-methods-use-this": 0,
          "no-useless-constructor": 0,
          "no-underscore-dangle": 0,
          "no-plusplus": 0,
          "no-param-reassign": 0,
          "no-empty-function": 0,
          "import/no-cycle": 0,
          "consistent-return": 0,
          "no-unused-vars": 0,
          "func-names": 0


    }
}
